<root>
  <styles>
    <include src="file://{resources}/styles/base.css" />
    <include src="file://{resources}/styles/hud/huddemoplayback.css" />
    <include src="file://{resources}/styles/csgostyles.css" />
  </styles>

  <CSGOHudDemoPlayback class="demoplayback">
    <Panel class="demoplayback-control-row">
      <Button class="demoplayback-control" onactivate="DemoPlaybackControl('Round', -9999)">
        <Label class="demoplayback-control-label" text="Restart"/>
      </Button>
      <Button class="demoplayback-control" onactivate="DemoPlaybackControl('Round', -1)">
        <Label class="demoplayback-control-label" text="Back"/>
      </Button>
      <Button class="demoplayback-control" onactivate="DemoPlaybackControl('Play', 0)">
        <Label class="demoplayback-control-label" text="Paws"/>
      </Button>
      <Button class="demoplayback-control" onactivate="DemoPlaybackControl('Play', 0.25)">
        <Label class="demoplayback-control-label" text="Slow"/>
      </Button>
      <Button class="demoplayback-control" onactivate="DemoPlaybackControl('Play', 1.0)">
        <Label class="demoplayback-control-label" text="Play"/>
      </Button>
      <Button class="demoplayback-control" onactivate="DemoPlaybackControl('Play', 4.0)">
        <Label class="demoplayback-control-label" text="Fast"/>
      </Button>
      <Button class="demoplayback-control" onactivate="DemoPlaybackControl('Round', 1)">
        <Label class="demoplayback-control-label" text="Next"/>
      </Button>
    </Panel>
    <Panel class="demoplayback-timer-row">
                      
      <Label text="{s:demo-time} / {s:demo-time-max}" />
    </Panel>
  </CSGOHudDemoPlayback>
</root>
